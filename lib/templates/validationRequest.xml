<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<Request xmlns='urn:oasis:names:tc:xacml:2.0:context:schema:os'>
<Subject SubjectCategory='urn:oasis:names:tc:xacml:1.0:subject-category:access-subject'>
    <Attribute AttributeId='urn:thalesgroup:xacml:oauth:token' DataType='http://www.w3.org/2001/XMLSchema#string'>
        <AttributeValue>{{subjectId}}</AttributeValue>
    </Attribute>
</Subject>
<Resource>
    <Attribute AttributeId='urn:oasis:names:tc:xacml:1.0:resource:resource-id'
               DataType='http://www.w3.org/2001/XMLSchema#string'>
        <AttributeValue>{{organization}}</AttributeValue>
    </Attribute>
</Resource>
<Action>
    <Attribute AttributeId='urn:oasis:names:tc:xacml:1.0:action:action-id'
               DataType='http://www.w3.org/2001/XMLSchema#string'>
        <AttributeValue>{{action}}</AttributeValue>
    </Attribute>
</Action>
<Environment/>
</Request>